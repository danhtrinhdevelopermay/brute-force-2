entrypoint = "main.py"
modules = ["python-3.8", "java-graalvm22.3"]

hidden = [".pythonlibs"]

[nix]
channel = "stable-23_05"
packages = ["unzip"]

[env]
LANG = "en_US.UTF-8"

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
