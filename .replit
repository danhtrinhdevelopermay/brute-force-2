args = "CPU GPU Monitor Info"

[[workflows.workflow]]
name = "CPU GPU Monitor Info"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "./show-project-info.sh"

[workflows.workflow.metadata]
outputType = "console"
